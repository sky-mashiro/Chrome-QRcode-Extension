{
  "manifest_version": 2,

  "name": "QRGenerater",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "2.0",

  "content_scripts": 
  [
    {
      "matches": ["https://*/*"],
      "js": [ "popup.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
